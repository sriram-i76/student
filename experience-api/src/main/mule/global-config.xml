	<http:request-config name="HTTP-Request-configuration-to-call-process-api" doc:name="HTTP Request configuration" doc:id="2d33e7bc-3f03-4117-bd47-f2b4891ef4ad">
		<http:request-connection protocol="${http.protocol}" host="${http.host}" port="${http.port}" />
	</http:request-config>
	 <apikit:config name="task-update-experience-api-config" api="task-update-experience-api.raml" outboundHeadersMapName="outboundHeaders" httpStatusVarName="httpStatus" />
	
</mule>
