		<http:request-connection protocol="${http.protocol}" host="${http.host}" port="${http.port}" />
	</http:request-config>
	<api-gateway:autodiscovery apiId="${api.id}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="f4323708-3a6b-432c-88a0-abbdbd947ecc" flowRef="student-experience-api-main" />
